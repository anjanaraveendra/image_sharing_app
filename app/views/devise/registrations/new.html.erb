<div align= "center" id="color">
  <h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="field" id="header">
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name, :id => "dim" %>
  </div>

  <div class="field" id="header">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name, :id => "dim" %>
  </div>

  <div class="field" id="header">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, :id => "dim" %>
  </div>

  <div class="field" id="header">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off", :id => "dim" %>
  </div>

  <div class="field" id="header">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off", :id => "dim" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
</div>