<div align= "center" id="color">
<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field" id="header">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, :id => "dim" %>
  </div>

  <div class="field" id="header">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off", :id => "dim"  %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field" id="header">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
</div>